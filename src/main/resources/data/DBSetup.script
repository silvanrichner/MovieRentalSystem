create table if not exists movies (	Id INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 100, INCREMENT BY 1) PRIMARY KEY, Title VARCHAR(255), IsRented BOOLEAN, ReleaseDate DATE, PriceCategory VARCHAR(20), AgeRating INTEGER );
create table if not exists clients ( Id INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 100, INCREMENT BY 1) PRIMARY KEY, Name VARCHAR(255), FirstName VARCHAR(255), Birthdate DATE );
create table if not exists rentals ( Id INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 100, INCREMENT BY 1) PRIMARY KEY, MovieId INTEGER, ClientId INTEGER, RentalDate DATE );
